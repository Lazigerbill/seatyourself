<img src="<%=@business_user[:picture_url]%>" alt="">
<%= form_for(@business_user) do |f| %>
<%= f.label :name %> </br>
<%= f.text_field :name %> </br>
<%= f.label :phone %> </br>
<%= f.text_field :phone %> </br>
<%= f.label :address %> </br>
<%= f.text_field :address %> </br>
<%= f.label :cuisine %> </br>
<%= f.text_field :cuisine %> </br>
<%= f.label :picture_url %> </br>
<%= f.text_field :picture_url %> </br>
<%= f.label :website %> </br>
<%= f.text_field :website %> </br>
<%= f.submit %> </br>

<% end %>